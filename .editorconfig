### Top-most EditorConfig file ###
root = true

### Default settings ###
# - Use 2 spaces for each indentation level
# - Use UTF-8
# - Use LF end of line sequence
# - Ensure files ends with a newline
# - Trim trailing whitespace
[*]
indent_size = 2
indent_style = space
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
